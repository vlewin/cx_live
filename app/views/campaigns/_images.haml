.panel.panel-default
  .panel-heading
    .panel-title
      Images
      .pull-right
        .badge= @images.size

  .panel-body
    - if @images
      .row
        - @images.each do |image|
          .col-xs-6.col-md-4
            = image_tag image.url, class: 'img-square img-circle img-thumbnail img-responsive'
    - else
      %h4 No images found!
